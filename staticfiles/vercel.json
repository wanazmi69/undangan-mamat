

{
  "builds": [
    {
      "src": "vercel_app/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles"
      }
    }

  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "project.settings"
  },
  "static": {
    "root": "staticfiles",
    "rewrites": false
  }
}

