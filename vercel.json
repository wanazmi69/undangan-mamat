

{
  "builds": [
    {
      "src": "vercel_app/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles"
      }
    },
    // { "src": "theme/static/css/dist/style.css", "use": "@vercel/static" },
    // { "src": "static/js/howler.js", "use": "@vercel/static" }
    // { "src": "static/js/howler.js", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app/wsgi.py"
    }
  ]
}

